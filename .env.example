# UBO Trace Engine Backend Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Database Configuration
MONGODB_URL=mongodb://username:password@host:port/database?options
DATABASE_NAME=ubo_trace_engine

# Application Settings
APP_NAME=UBO Trace Engine Backend
DEBUG=True
VERSION=1.0.0

# Lyzr AI Configuration
LYZR_API_URL=https://agent-prod.studio.lyzr.ai/v3/inference/chat/
LYZR_API_KEY=your_lyzr_api_key_here
LYZR_USER_ID=your_lyzr_user_id_here

# Agent IDs for different stages
AGENT_STAGE_1A=your_agent_stage_1a_id_here
AGENT_STAGE_1B=your_agent_stage_1b_id_here
AGENT_STAGE_2A=your_agent_stage_2a_id_here
AGENT_STAGE_2B=your_agent_stage_2b_id_here

# Session IDs for different stages
SESSION_STAGE_1A=your_session_stage_1a_id_here
SESSION_STAGE_1B=your_session_stage_1b_id_here
SESSION_STAGE_2A=your_session_stage_2a_id_here
SESSION_STAGE_2B=your_session_stage_2b_id_here

# Company Domain Analysis Agent
AGENT_COMPANY_DOMAIN=your_agent_company_domain_id_here
SESSION_COMPANY_DOMAIN=your_session_company_domain_id_here

# Expert Domain Analysis Agent
AGENT_EXPERT_DOMAIN=your_agent_expert_domain_id_here
SESSION_EXPERT_DOMAIN=your_session_expert_domain_id_here

# UBO Search Agents
AGENT_UBO_DOMAIN=your_agent_ubo_domain_id_here
SESSION_UBO_DOMAIN=your_session_ubo_domain_id_here
AGENT_UBO_CSUITE=your_agent_ubo_csuite_id_here
SESSION_UBO_CSUITE=your_session_ubo_csuite_id_here
AGENT_UBO_UBOS=your_agent_ubo_ubos_id_here
SESSION_UBO_UBOS=your_session_ubo_ubos_id_here
AGENT_UBO_CROSSVERIFY=your_agent_ubo_crossverify_id_here
SESSION_UBO_CROSSVERIFY=your_session_ubo_crossverify_id_here
AGENT_UBO_REGISTRIES=your_agent_ubo_registries_id_here
SESSION_UBO_REGISTRIES=your_session_ubo_registries_id_here
AGENT_UBO_HIERARCHY=your_agent_ubo_hierarchy_id_here
SESSION_UBO_HIERARCHY=your_session_ubo_hierarchy_id_here

# UBO Ownership Analysis Agent (for Google Search results)
AGENT_UBO_OWNERSHIP_ANALYSIS=your_agent_ubo_ownership_analysis_id_here
SESSION_UBO_OWNERSHIP_ANALYSIS=your_session_ubo_ownership_analysis_id_here

# Apollo People Search Analysis Agent (for Apollo API results)
AGENT_APOLLO_PEOPLE_ANALYSIS=your_agent_apollo_people_analysis_id_here
SESSION_APOLLO_PEOPLE_ANALYSIS=your_session_apollo_people_analysis_id_here

# Apollo.io API settings
APOLLO_API_KEY=your_apollo_api_key_here
APOLLO_BASE_URL=https://api.apollo.io/v1
APOLLO_TIMEOUT=30

# SearchAPI Google settings
SEARCHAPI_API_KEY=your_searchapi_api_key_here
SEARCHAPI_TIMEOUT=30

# API Settings
API_TIMEOUT=60
MAX_RETRIES=3
RETRY_DELAY=5.0
RATE_LIMIT_PER_MINUTE=10

# Candidate UBO Analysis Agent
AGENT_CANDIDATE_UBO_ANALYSIS=your_agent_candidate_ubo_analysis_id_here
SESSION_CANDIDATE_UBO_ANALYSIS=your_session_candidate_ubo_analysis_id_here
